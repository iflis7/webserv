
// Multiple Servers


// Multiple Ports 
server {
    listen       80;
    server_name  example.org  www.example.org;
    root         /var/www/port80/
}

server {
    listen       81;
    server_name  example.org  www.example.org;
    root         /var/www/port81/
}

server {
    listen 80;
    listen 8000;
    server_name example.org;
    root /var/www/;
}


// Multiple Domains






 