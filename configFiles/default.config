server {
	listen 127.0.0.1:8080;
	server_name  	webserv;
	methods 		GET POST DELETE;
	error_page		400 400.html;
	error_page		405 405.html;
	error_page		404 404.html;
	client_size		10;

	location / {

		index	 index.html;
	}

	location /upload {
		root 	/data/tmp/;
		index	 index.html;
		methods 		POST;
		redirection		/telecharger;
		error_page		400 400v2.html
	}

	location /toto {
		root	 /data/www/toto;
	}
}
