server {
	listen 127.0.0.1:8080
	server_name server1;
	root  data/www;
	index index.html;

	error_page		400 error_pages/400.html;
	error_page		404 error_pages/404.html;
	error_page		405 error_pages/405.html;
	methods 		GET     POST    DELETE;

	location /tata {
        root data/www/toto;
        index index.html;
        autoindex ON;
    }

    location /cgi {
		root data/cgi;
		index index.html;
	}
}
